<div id="sessions-new">
  <%= link_to "Sign in with Soundcloud", "/auth/soundcloud" %>
  <div class="form">
    <%= form_for sessions_path do %>
    <div class="username">
      <%= label_tag :username %>
      <%= text_field_tag :username %>
    </div>
    <div class="password">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div class="submit">
      <%= submit_tag(["Sign in"]) %>
    </div>
    <% end %>
  </div>
</div>
